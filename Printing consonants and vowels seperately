word = "Engineering"
vowels_list = []
consonants_list = []
for ch in word:
    if ch.isalpha():
        if ch.lower() in 'aeiou':
            vowels_list.append(ch)
        else:
            consonants_list.append(ch)
print("Consonants:", ' '.join(consonants_list))
print("Vowels:", ' '.join(vowels_list))
